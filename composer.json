{
    "name": "efficiently/jquery-laravel",
    "description": "This package provides jQuery and the jQuery-ujs driver for your Laravel 5.0 application.",
    "keywords": [
        "jquery",
        "laravel",
        "laravel 5",
        "larasset",
        "assets",
        "asset pipeline",
        "pipeline",
        "javascript",
        "ujs",
        "unobtrusive",
        "ajax"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Tortue Torche",
            "email": "tortuetorche@spam.me"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~5.0.0||~5.1.0 ",
        "laravelcollective/html": "~5.0.0||~5.1.0"
    },
    "require-dev": {
      "phpunit/phpunit": "~4.5",
      "mockery/mockery": "0.9.*",
      "orchestra/testbench": "~3.0.0||~3.1.0",
      "anahkiasen/former": "~4.0.0"
    },
    "autoload": {
        "files": [
            "src/Efficiently/JqueryLaravel/helpers.php"
        ],
        "psr-0": {
            "Efficiently\\JqueryLaravel\\": "src/"
        }
    },
    "autoload-dev": {
      "classmap": [
        "tests/JqlTestCase.php"
      ],
      "psr-4": {
        "Jql\\": "tests/fixtures/jql/"
      }
    },
    "config": {
        "preferred-install": "dist"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
